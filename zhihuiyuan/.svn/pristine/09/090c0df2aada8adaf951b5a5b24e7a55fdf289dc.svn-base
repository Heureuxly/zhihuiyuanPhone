@charest "UTF-8";

body, ul, li, p { margin: 0; padding: 0; }
ul, li { list-style: none; }
a { text-decoration: none; }
body{
    background:#afcae8;
}

.clear { zoom: 1; }
.clear:after { content: ''; display: block; clear: both; }


#header { width: 900px; height: 105px; margin: 0 auto; background: url(../img/head_bg.jpg) no-repeat; }
.nav_list li { float: left; width: 180px; height: 27px; line-height: 27px; text-align: center; }
.nav_list li a { font-size: 12px; color: #295369;}
.nav_list li ul { display: none; }
.nav_list li ul li { background: #ccc; border-bottom: 1px solid #fff; text-align: center; }


#contain { width: 900px; margin: 0 auto; background: #fff; }
#contain .contain_nav { width: 100%; height: 25px; border-bottom: 1px solid #c3c5d1; font-size: 12px; color: #000; line-height: 25px; text-indent: 15px; }
#contain .contain_side { float: left; width: 130px; height: 500px; background: #d4d6ff; font-size: 12px; padding: 15px 10px ; }
